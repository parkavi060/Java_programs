##to find two arrays are equal or not;

import java.util.*;
public class Main
{
	public static void main(String[] args) {
		int a[]={1,2,3,4,5,6};
		int b[]={7,8,9,5,3,4};
		Arrays.sort(a);
		Arrays.sort(b);
		if(Arrays.equals(a,b))
		{
		    System.out.print("The two arrays are equal");
		}
		else {
		    System.out.print("Not equal");
		}
	}
}

